{
  "$id": "ce_article",
  "$schema": "https://schema.yext.com/config/km/entity-type/v1",
  "description": "An insights article from UBS Wealth Management",
  "displayName": "${{zendeskSubdomain}}",
  "enabled": true,
  "field": [
    {
      "id": "${{businessId}}",
      "required": false
    },
    {
      "id": "yext/name",
      "required": true
    },
    {
      "id": "yext/categories",
      "required": false
    },
    {
      "id": "yext/folder",
      "required": false
    },
    {
      "id": "yext/labels",
      "required": false
    },
    {
      "id": "yext/entityId",
      "required": true
    }
  ],
  "pluralDisplayName": "${{autoGenerateSitemap}}"
}

